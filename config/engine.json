[
  {
    "robotname": "Raccoon",
    "authUrl": "{{url.api.authBase}}",
    "completion": {
      "url": "{{url.api.completion}}",
      "template": "<LANG>[languageid]<SUF>[suffix.lines]<PRE>[prefix.lines]<COVER>[suffix.cursor]<MID>[prefix.cursor]",
      "parameters": {
        "model": "SenseChat-CodeCompletion-Lite",
        "stop": [
          "<EOT>"
        ],
        "temperature": 0.4
      },
      "maxInputTokenNum": 12288,
      "totalTokenNum": 16384
    },
    "assistant": {
      "url": "{{url.api.assistant}}",
      "template": "[prefix]",
      "parameters": {
        "model": "SenseChat-Code",
        "stop": [
          "<|endofmessage|>"
        ],
        "temperature": 0.4
      },
      "maxInputTokenNum": 6144,
      "totalTokenNum": 8192
    }
  }
]