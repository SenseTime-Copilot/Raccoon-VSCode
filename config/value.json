{
    "emailLogin": false,
    "signup": "https://raccoon.sensetime.com/register",
    "forgetPassword": "https://raccoon.sensetime.com/login?step=forgot-password",
    "docs": "https://raccoon.sensetime.com/code/docs",
    "engines": [
        {
            "robotname": "Raccoon",
            "apiBaseUrl": "https://raccoon-api.sensetime.com/api/plugin",
            "completion": {
                "template": "<LANG>[languageid]<SUF>[suffix.lines]<PRE>[prefix.lines]<COVER>[suffix.cursor]<MID>[prefix.cursor]",
                "parameters": {
                    "model": "SenseChat-CodeCompletion-Lite",
                    "stop": [
                        "<EOT>"
                    ],
                    "temperature": 0.4
                },
                "maxInputTokenNum": 12288,
                "totalTokenNum": 16384
            },
            "assistant": {
                "template": "[prefix]",
                "parameters": {
                    "model": "SenseChat-Code",
                    "stop": [
                        "<|endofmessage|>"
                    ],
                    "temperature": 0.4
                },
                "maxInputTokenNum": 6144,
                "totalTokenNum": 8192
            }
        }
    ]
}