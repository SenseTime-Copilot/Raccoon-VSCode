{
    "publisher": "SenseTime",
    "name": "sensecode",
    "displayName": "Sense Code",
    "description": "Sense Code is an AI-based coding assistant.",
    "homepage": "https://sensecore.cn",
    "repository": {
        "type": "git",
        "url": "https://gitlab.bj.sensetime.com/elementary/studio/sensecode-vscode-extension.git"
    },
    "icon": "media/sensecode-logo.png",
    "version": "0.8.4",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "python",
        "go",
        "java",
        "javascript",
        "c++",
        "c#",
        "php",
        "typescript",
        "ai",
        "autocomplete",
        "inline completion"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": [
            {
                "properties": {
                    "SenseCode.Engines": {
                        "type": "array",
                        "description": "Engine List",
                        "items": {
                            "type": "object",
                            "description": "Engine Config",
                            "properties": {
                                "label": {
                                    "type": "string",
                                    "description": "Engine label for UI dispaly"
                                },
                                "url": {
                                    "type": "string",
                                    "description": "Engine API URL"
                                },
                                "capacities": {
                                    "type": "array",
                                    "description": "Engine capacity list",
                                    "items": {
                                        "type": "string",
                                        "enum": [
                                            "completion",
                                            "chat"
                                        ]
                                    },
                                    "uniqueItems": true
                                },
                                "key": {
                                    "type": "string",
                                    "description": "API Key for engine"
                                },
                                "config": {
                                    "type": "object",
                                    "description": "Request config"
                                }
                            }
                        }
                    },
                    "SenseCode.CompletionAutomatically": {
                        "type": "boolean",
                        "default": false,
                        "description": "Automatic get completion suggestions once stop typing."
                    },
                    "SenseCode.DirectPrintOut": {
                        "type": "boolean",
                        "default": false,
                        "description": "Direct print completion code to editor."
                    },
                    "SenseCode.CompletionDelay": {
                        "type": "number",
                        "minimum": 0.5,
                        "default": 3,
                        "description": "The delay in seconds to start getting completion suggestions. You can change this value to get better experience in coding with our extension and to avoid some unuseful completion suggestions."
                    },
                    "SenseCode.Prompt": {
                        "type": "object",
                        "description": "Prompt templates",
                        "additionalProperties": {
                            "type": "string",
                            "description": "Prompt template"
                        },
                        "default": {
                            "addTests": "Implement tests for the following code",
                            "findProblems": "Find problems with the following code",
                            "optimize": "Optimize the following code",
                            "explain": "Explain the following code"
                        }
                    },
                    "SenseCode.Next": {
                        "type": "object"
                    }
                }
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "sensecode.settings",
                    "group": "navigation@0",
                    "when": "view == sensecode.view"
                }
            ],
            "editor/context": [
                {
                    "command": "sensecode.addTests",
                    "group": "SenseCode@1",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.findProblems",
                    "group": "SenseCode@2",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.optimize",
                    "group": "SenseCode@3",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.explain",
                    "group": "SenseCode@4",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.customPrompt",
                    "group": "SenseCode@5",
                    "when": "editorHasSelection && sensecode.next.chat"
                }
            ]
        },
        "commands": [
            {
                "command": "sensecode.settings",
                "category": "SenseCode",
                "title": "Settings",
                "icon": "$(gear)"
            },
            {
                "command": "sensecode.clearKey",
                "category": "SenseCode",
                "title": "Clear Key"
            },
            {
                "command": "sensecode.addTests",
                "title": "SenseCode: Add tests",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.findProblems",
                "title": "SenseCode: Find bugs",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.optimize",
                "title": "SenseCode: Optimize",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.explain",
                "title": "SenseCode: Explain",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.customPrompt",
                "title": "SenseCode: Custom Prompt...",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.config.selectEngine",
                "category": "SenseCode",
                "title": "Select Code Completion Engine"
            },
            {
                "command": "sensecode.inlineSuggest.toggleAuto",
                "category": "SenseCode",
                "title": "Toggle Auto/Manual Code Inline Completion"
            },
            {
                "command": "sensecode.inlineSuggest.togglePrintOut",
                "category": "SenseCode",
                "title": "Toggle Snippets/Print Code Inline Completion"
            },
            {
                "command": "sensecode.inlineSuggest.trigger",
                "category": "SenseCode",
                "title": "Code Inline Completion Trigger"
            }
        ],
        "keybindings": [
            {
                "command": "sensecode.inlineSuggest.trigger",
                "key": "Alt+/"
            }
        ],
        "icons": {
            "sensecore-dark": {
                "description": "SenseCore icon",
                "default": {
                    "fontPath": "media/penrose.woff",
                    "fontCharacter": "\\E001"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "sensecode-view-container",
                    "title": "SenseCode",
                    "icon": "media/logo1.svg"
                }
            ]
        },
        "views": {
            "sensecode-view-container": [
                {
                    "type": "webview",
                    "id": "sensecode.view",
                    "name": "Assistant"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint -c .eslintrc.js --ext .ts ./src --fix",
        "vscode:prepublish": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "@vscode/webview-ui-toolkit": "^1.2.0",
        "@vscode/vsce": "^2.18.0",
        "axios": "^0.24.0",
        "crypto": "^1.0.1",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "ts-loader": "^9.4.1",
        "ts-webpack": "^1.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    }
}