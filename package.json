{
    "publisher": "SenseTime",
    "name": "sensecode",
    "displayName": "Sense Code",
    "description": "%sensecode.description%",
    "homepage": "https://sensecore.cn",
    "repository": {
        "type": "git",
        "url": "https://gitlab.bj.sensetime.com/elementary/studio/sensecode-vscode-extension.git"
    },
    "icon": "media/sensecode-logo.png",
    "version": "0.8.6",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "python",
        "go",
        "java",
        "javascript",
        "c++",
        "c#",
        "php",
        "typescript",
        "ai",
        "autocomplete",
        "inline completion"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "l10n": "./l10n",
    "contributes": {
        "configuration": [
            {
                "properties": {
                    "SenseCode.Engines": {
                        "type": "array",
                        "description": "%sensecode.config.engines.description%",
                        "items": {
                            "type": "object",
                            "description": "%sensecode.config.engines.engine.description%",
                            "properties": {
                                "label": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.label%"
                                },
                                "url": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.url%"
                                },
                                "capacities": {
                                    "type": "array",
                                    "description": "%sensecode.config.engines.engine.capacities%",
                                    "items": {
                                        "type": "string",
                                        "enum": [
                                            "completion",
                                            "chat"
                                        ]
                                    },
                                    "uniqueItems": true
                                },
                                "key": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.key%"
                                },
                                "config": {
                                    "type": "object",
                                    "description": "%sensecode.config.engines.engine.config%"
                                }
                            }
                        }
                    },
                    "SenseCode.CompletionAutomatically": {
                        "type": "boolean",
                        "default": false,
                        "description": "%sensecode.config.autoCompletion%"
                    },
                    "SenseCode.CompletionDelay": {
                        "type": "number",
                        "minimum": 0.5,
                        "default": 3,
                        "description": "%sensecode.config.completionDelay%"
                    },
                    "SenseCode.DirectPrintOut": {
                        "type": "boolean",
                        "default": false,
                        "description": "%sensecode.config.printOut%"
                    },
                    "SenseCode.Prompt": {
                        "type": "object",
                        "description": "%sensecode.config.prompt%",
                        "additionalProperties": {
                            "type": "string",
                            "description": "%sensecode.config.prompt.item%"
                        },
                        "default": {
                            "addTests": "%sensecode.command.addTests.prompt%",
                            "findProblems": "%sensecode.command.findProblems.prompt%",
                            "optimize": "%sensecode.command.optimize.prompt%",
                            "explain": "%sensecode.command.explain.prompt%"
                        }
                    },
                    "SenseCode.Next": {
                        "type": "object"
                    }
                }
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "sensecode.settings",
                    "group": "navigation@0",
                    "when": "view == sensecode.view"
                }
            ],
            "editor/context": [
                {
                    "command": "sensecode.addTests",
                    "group": "SenseCode@1",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.findProblems",
                    "group": "SenseCode@2",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.optimize",
                    "group": "SenseCode@3",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.explain",
                    "group": "SenseCode@4",
                    "when": "editorHasSelection && sensecode.next.chat"
                },
                {
                    "command": "sensecode.customPrompt",
                    "group": "SenseCode@5",
                    "when": "editorHasSelection && sensecode.next.chat"
                }
            ]
        },
        "commands": [
            {
                "command": "sensecode.settings",
                "category": "SenseCode",
                "title": "%sensecode.command.settings%",
                "icon": "$(gear)"
            },
            {
                "command": "sensecode.clearKey",
                "category": "SenseCode",
                "title": "%sensecode.command.clearKey%"
            },
            {
                "command": "sensecode.addTests",
                "title": "%sensecode.command.addTests%",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.findProblems",
                "title": "%sensecode.command.findProblems%",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.optimize",
                "title": "%sensecode.command.optimize%",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.explain",
                "title": "%sensecode.command.explain%",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.customPrompt",
                "title": "%sensecode.command.customPrompt%",
                "enablement": "editorHasSelection"
            },
            {
                "command": "sensecode.config.selectEngine",
                "category": "SenseCode",
                "title": "%sensecode.command.selectEngine%"
            },
            {
                "command": "sensecode.inlineSuggest.toggleAuto",
                "category": "SenseCode",
                "title": "%sensecode.command.toggleAutoCompletion%"
            },
            {
                "command": "sensecode.inlineSuggest.togglePrintOut",
                "category": "SenseCode",
                "title": "%sensecode.command.toggleCompletionMode%"
            },
            {
                "command": "sensecode.inlineSuggest.trigger",
                "category": "SenseCode",
                "title": "%sensecode.command.completionTrigger%"
            },
            {
                "command": "sensecode.inlineSuggest.setKeybinding",
                "category": "SenseCode",
                "title": "%sensecode.command.setKeyinding%"
            }
        ],
        "keybindings": [
            {
                "command": "sensecode.inlineSuggest.trigger",
                "key": "Alt+/"
            }
        ],
        "icons": {
            "sensecore-dark": {
                "description": "SenseCore icon",
                "default": {
                    "fontPath": "media/penrose.woff",
                    "fontCharacter": "\\E001"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "sensecode-view-container",
                    "title": "SenseCode",
                    "icon": "media/logo1.svg"
                }
            ]
        },
        "views": {
            "sensecode-view-container": [
                {
                    "type": "webview",
                    "id": "sensecode.view",
                    "name": "%sensecode.view.title%"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint -c .eslintrc.js --ext .ts ./src --fix",
        "vscode:prepublish": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "@vscode/webview-ui-toolkit": "^1.2.0",
        "@vscode/vsce": "^2.18.0",
        "axios": "^0.24.0",
        "crypto": "^1.0.1",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "ts-loader": "^9.4.1",
        "ts-webpack": "^1.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    }
}