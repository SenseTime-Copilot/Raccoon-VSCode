{
    "publisher": "SenseTime",
    "name": "sensecode",
    "displayName": "SenseCode",
    "description": "%sensecode.description%",
    "homepage": "https://sensecode.ai",
    "repository": {
        "type": "git",
        "url": "https://github.com/SenseCore/SenseCode-VSCode.git"
    },
    "bugs": {
        "url": "https://github.com/SenseCore/SenseCode-VSCode/issues"
    },
    "icon": "media/sensecode-logo.png",
    "version": "0.60.0",
    "preview": true,
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "ai",
        "autocomplete",
        "inline completion"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onUri",
        "workspaceContains:**/*"
    ],
    "main": "./dist/extension.js",
    "browser": "./dist/web/extension.js",
    "l10n": "./l10n",
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "extensionDependencies": [
        "vscode.git"
    ],
    "contributes": {
        "configuration": [
            {
                "properties": {
                    "SenseCode.Engines": {
                        "type": "array",
                        "description": "%sensecode.config.engines.description%",
                        "items": {
                            "type": "object",
                            "description": "%sensecode.config.engines.engine.description%",
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.type%",
                                    "enum": [
                                        "sensecore",
                                        "sensenova",
                                        "openai",
                                        "tgi"
                                    ]
                                },
                                "robotname": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.label%",
                                    "minLength": 1,
                                    "maxLength": 32
                                },
                                "username": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.username%",
                                    "minLength": 1,
                                    "maxLength": 32
                                },
                                "authUrl": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.authurl%",
                                    "format": "uri"
                                },
                                "key": {
                                    "description": "%sensecode.config.engines.engine.key%",
                                    "oneOf": [
                                        {
                                            "type": "string"
                                        },
                                        {
                                            "type": "object",
                                            "properties": {
                                                "accessKeyId": {
                                                    "type": "string",
                                                    "description": "%sensecode.config.engines.engine.key.ak%"
                                                },
                                                "secretAccessKey": {
                                                    "type": "string",
                                                    "description": "%sensecode.config.engines.engine.key.sk%"
                                                }
                                            }
                                        }
                                    ]
                                },
                                "completion": {
                                    "type": "object",
                                    "description": "%sensecode.config.engines.engine.completion%",
                                    "properties": {
                                        "url": {
                                            "type": "string",
                                            "description": "%sensecode.config.engines.engine.url%",
                                            "format": "uri"
                                        },
                                        "template": {
                                            "type": "string",
                                            "description": "%sensecode.config.engines.engine.option.template%"
                                        },
                                        "parameters": {
                                            "type": "object",
                                            "markdownDescription": "%sensecode.config.engines.engine.option.parameters%",
                                            "properties": {
                                                "model": {
                                                    "type": "string"
                                                },
                                                "stop": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                },
                                                "temperature": {
                                                    "type": "number"
                                                }
                                            }
                                        },
                                        "totalTokenNum": {
                                            "type": "number",
                                            "minimum": 1,
                                            "description": "%sensecode.config.engines.engine.option.total_token_num%"
                                        },
                                        "maxInputTokenNum": {
                                            "type": "number",
                                            "minimum": 1,
                                            "description": "%sensecode.config.engines.engine.option.max_input_token_num%"
                                        }
                                    },
                                    "required": [
                                        "url",
                                        "template"
                                    ],
                                    "additionalProperties": false
                                },
                                "assistant": {
                                    "type": "object",
                                    "description": "%sensecode.config.engines.engine.assistant%",
                                    "properties": {
                                        "url": {
                                            "type": "string",
                                            "description": "%sensecode.config.engines.engine.url%",
                                            "format": "uri"
                                        },
                                        "template": {
                                            "type": "string",
                                            "description": "%sensecode.config.engines.engine.option.template%"
                                        },
                                        "parameters": {
                                            "type": "object",
                                            "markdownDescription": "%sensecode.config.engines.engine.option.parameters%",
                                            "properties": {
                                                "model": {
                                                    "type": "string"
                                                },
                                                "stop": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                },
                                                "temperature": {
                                                    "type": "number"
                                                }
                                            }
                                        },
                                        "totalTokenNum": {
                                            "type": "number",
                                            "minimum": 1,
                                            "description": "%sensecode.config.engines.engine.option.total_token_num%"
                                        },
                                        "maxInputTokenNum": {
                                            "type": "number",
                                            "minimum": 1,
                                            "description": "%sensecode.config.engines.engine.option.max_input_token_num%"
                                        }
                                    },
                                    "required": [
                                        "url",
                                        "template"
                                    ],
                                    "additionalProperties": false
                                }
                            },
                            "required": [
                                "type",
                                "robotname"
                            ],
                            "additionalProperties": false
                        }
                    },
                    "SenseCode.Prompt": {
                        "type": "object",
                        "markdownDescription": "%sensecode.config.prompt%",
                        "additionalProperties": {
                            "oneOf": [
                                {
                                    "type": "object",
                                    "description": "%sensecode.config.prompt.advanced%",
                                    "properties": {
                                        "prompt": {
                                            "type": "string",
                                            "markdownDescription": "%sensecode.config.prompt.advanced.item%"
                                        },
                                        "shortcut": {
                                            "type": "string",
                                            "description": "%sensecode.config.prompt.advanced.shortcut%",
                                            "pattern": "^[_a-zA-Z][_a-zA-Z0-9]{0,15}$"
                                        },
                                        "args": {
                                            "type": "object",
                                            "description": "%sensecode.config.prompt.advanced.args%",
                                            "patternProperties": {
                                                "^.+$": {
                                                    "type": "object",
                                                    "properties": {
                                                        "type": {
                                                            "type": "string",
                                                            "markdownDescription": "%sensecode.config.prompt.advanced.args.type%",
                                                            "enum": [
                                                                "text",
                                                                "range",
                                                                "date",
                                                                "datetime-local",
                                                                "month",
                                                                "week",
                                                                "number",
                                                                "tel",
                                                                "url",
                                                                "email",
                                                                "color"
                                                            ]
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "required": [
                                        "prompt"
                                    ]
                                },
                                {
                                    "type": "string",
                                    "description": "%sensecode.config.prompt.item%"
                                }
                            ]
                        }
                    },
                    "SenseCode.Dev": {
                        "type": "object"
                    }
                }
            }
        ],
        "submenus": [
            {
                "id": "sensecode-submenu",
                "label": "%sensecode.submenu%",
                "icon": "$(more)"
            }
        ],
        "menus": {
            "file/newFile": [
                {
                    "command": "sensecode.notebook.new"
                }
            ],
            "editor/context": [
                {
                    "command": "sensecode.ask",
                    "group": "z_commands",
                    "when": "editorTextFocus"
                }
            ],
            "scm/title": [
                {
                    "command": "sensecode.commit-msg",
                    "group": "navigation@1000",
                    "when": "config.git.enabled && !git.missing && scmProvider == git && gitOpenRepositoryCount != 0"
                }
            ],
            "sensecode-submenu": [
                {
                    "command": "sensecode.terminal",
                    "group": "navigation@3",
                    "when": "view == sensecode.view"
                },
                {
                    "command": "sensecode.notebook.new",
                    "group": "navigation@4",
                    "when": "view == sensecode.view"
                },
                {
                    "command": "sensecode.favorite.manage",
                    "group": "cache@0",
                    "when": "view == sensecode.view"
                },
                {
                    "command": "sensecode.help",
                    "group": "help@0",
                    "when": "view == sensecode.view"
                }
            ],
            "view/title": [
                {
                    "command": "sensecode.clear",
                    "group": "navigation@1",
                    "when": "view == sensecode.view"
                },
                {
                    "command": "sensecode.openEditor",
                    "group": "navigation@2",
                    "when": "view == sensecode.view"
                },
                {
                    "command": "sensecode.settings",
                    "group": "navigation@3",
                    "when": "view == sensecode.view"
                },
                {
                    "submenu": "sensecode-submenu",
                    "group": "navigation@4",
                    "when": "view == sensecode.view"
                }
            ],
            "editor/title": [
                {
                    "command": "sensecode.clear",
                    "group": "navigation@1",
                    "when": "resourceFilename == 'assistant.sensecode'"
                }
            ]
        },
        "commands": [
            {
                "command": "sensecode.settings",
                "category": "SenseCode",
                "title": "%sensecode.command.settings%",
                "icon": "$(gear)",
                "enablement": "view == sensecode.view"
            },
            {
                "command": "sensecode.help",
                "category": "SenseCode",
                "title": "%sensecode.command.help%",
                "icon": "$(question)",
                "enablement": "view == sensecode.view"
            },
            {
                "command": "sensecode.favorite.manage",
                "category": "SenseCode",
                "title": "%sensecode.command.favorite%",
                "icon": "$(repo)",
                "enablement": "view == sensecode.view"
            },
            {
                "command": "sensecode.terminal",
                "category": "SenseCode",
                "title": "%sensecode.command.terminal%",
                "icon": "$(terminal)",
                "enablement": "view == sensecode.view"
            },
            {
                "command": "sensecode.clear",
                "category": "SenseCode",
                "title": "%sensecode.command.clear%",
                "icon": "$(clear-all)",
                "enablement": "view == sensecode.view || resourceFilename == 'assistant.sensecode'"
            },
            {
                "command": "sensecode.ask",
                "category": "SenseCode",
                "title": "%sensecode.command.ask%"
            },
            {
                "command": "sensecode.commit-msg",
                "category": "SenseCode",
                "title": "%sensecode.command.commit-msg%",
                "icon": "$(wand)",
                "enablement": "view == workbench.scm"
            },
            {
                "command": "sensecode.settings.reset",
                "category": "SenseCode",
                "title": "%sensecode.command.settingsReset%"
            },
            {
                "command": "sensecode.openEditor",
                "category": "SenseCode",
                "title": "%sensecode.command.openEditor%",
                "icon": "$(empty-window)",
                "enablement": "view == sensecode.view || resourceFilename == 'assistant.sensecode'"
            },
            {
                "command": "sensecode.inlineSuggest.trigger",
                "category": "SenseCode",
                "title": "%sensecode.command.completionTrigger%",
                "enablement": "editorTextFocus && !editorReadonly"
            },
            {
                "command": "sensecode.onSuggestionAccepted",
                "category": "SenseCode",
                "title": "%sensecode.command.onSuggestionAccepted%",
                "enablement": "editorTextFocus && !editorReadonly"
            },
            {
                "command": "sensecode.notebook.new",
                "category": "SenseCode",
                "title": "%sensecode.command.notebook.new%",
                "icon": "$(notebook)"
            }
        ],
        "keybindings": [
            {
                "command": "sensecode.inlineSuggest.trigger",
                "key": "Alt+/"
            },
            {
                "command": "sensecode.ask",
                "key": "ctrl ctrl"
            }
        ],
        "icons": {
            "sensecode-icon": {
                "description": "SenseCode icon",
                "default": {
                    "fontPath": "media/sensecode.woff",
                    "fontCharacter": "\\E001"
                }
            }
        },
        "languages": [
            {
                "id": "sensecode",
                "extensions": [
                    ".sensecode",
                    ".favorites",
                    ".scnb",
                    ".search",
                    ".question"
                ],
                "icon": {
                    "light": "media/sensecode-logo.png",
                    "dark": "media/sensecode-logo.png"
                }
            }
        ],
        "notebooks": [
            {
                "type": "sensecode",
                "displayName": "SenseCode Notebook",
                "selector": [
                    {
                        "filenamePattern": "*.scnb"
                    }
                ]
            }
        ],
        "resourceLabelFormatters": [
            {
                "scheme": "sensecode",
                "authority": "sensecode.favorites",
                "formatting": {
                    "label": "${query.title}"
                }
            },
            {
                "scheme": "sensecode",
                "authority": "sensecode.editor",
                "formatting": {
                    "label": "%sensecode.view.title%"
                }
            },
            {
                "scheme": "sensecode",
                "authority": "sensecode.search",
                "formatting": {
                    "label": "${query.query}"
                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "sensecode-view-container",
                    "title": "SenseCode",
                    "icon": "media/sensecode.svg"
                }
            ]
        },
        "views": {
            "sensecode-view-container": [
                {
                    "type": "webview",
                    "id": "sensecode.view",
                    "name": "%sensecode.view.title%"
                }
            ]
        },
        "customEditors": [
            {
                "viewType": "sensecode.favorites",
                "displayName": "SenseCode Favorites",
                "selector": [
                    {
                        "filenamePattern": "*.sensecode.favorites"
                    }
                ]
            },
            {
                "viewType": "sensecode.editor",
                "displayName": "SenseCode Assistant",
                "selector": [
                    {
                        "filenamePattern": "*.sensecode"
                    }
                ]
            },
            {
                "viewType": "sensecode.search",
                "displayName": "SenseCode Search Result",
                "selector": [
                    {
                        "filenamePattern": "*.sensecode.search"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "lint": "eslint -c .eslintrc.js --ext .ts ./src --fix",
        "vscode:prepublish": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack --mode development",
        "test-compile": "tsc -p ./",
        "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.75.0",
        "@types/jwt-encode": "^1.0.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "@vscode/vsce": "^2.18.0",
        "@vscode/test-web": "^0.0.44",
        "@vscode/webview-ui-toolkit": "^1.2.0",
        "langchain": "^0.0.116",
        "axios": "^0.24.0",
        "crypto": "^1.0.1",
        "crypto-browserify": "^3.12.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "https-browserify": "^1.0.0",
        "url": "^0.11.1",
        "process": "^0.11.10",
        "eslint": "^8.20.0",
        "form-data": "^4.0.0",
        "glob": "^8.0.3",
        "jwt-decode": "^3.1.2",
        "jwt-encode": "^1.0.1",
        "mocha": "^10.0.0",
        "ts-loader": "^9.4.1",
        "ts-webpack": "^1.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "zlib": "^1.0.5"
    }
}