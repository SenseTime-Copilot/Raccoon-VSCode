{
    "publisher": "SenseTime",
    "name": "sensecode",
    "displayName": "Sense Code",
    "description": "%sensecode.description%",
    "homepage": "https://sensecore.cn",
    "repository": {
        "type": "git",
        "url": "https://gitlab.bj.sensetime.com/elementary/studio/sensecode-vscode-extension.git"
    },
    "icon": "media/sensecode-logo.png",
    "version": "0.16.0",
    "preview": true,
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "ai",
        "autocomplete",
        "inline completion"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "l10n": "./l10n",
    "contributes": {
        "configuration": [
            {
                "properties": {
                    "SenseCode.Engines": {
                        "type": "array",
                        "description": "%sensecode.config.engines.description%",
                        "items": {
                            "type": "object",
                            "description": "%sensecode.config.engines.engine.description%",
                            "properties": {
                                "label": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.label%",
                                    "pattern": "^(?!Penrose$)"
                                },
                                "url": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.url%"
                                },
                                "key": {
                                    "type": "string",
                                    "description": "%sensecode.config.engines.engine.key%"
                                },
                                "config": {
                                    "type": "object",
                                    "description": "%sensecode.config.engines.engine.config%"
                                }
                            }
                        }
                    },
                    "SenseCode.Prompt": {
                        "type": "object",
                        "description": "%sensecode.config.prompt%",
                        "additionalProperties": {
                            "type": "string",
                            "description": "%sensecode.config.prompt.item%"
                        }
                    }
                }
            }
        ],
        "customEditors": [
            {
                "viewType": "sensecode.correction",
                "displayName": "Code Correction",
                "priority": "default"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "sensecode.settings",
                    "group": "navigation@0",
                    "when": "view == sensecode.view"
                }
            ]
        },
        "commands": [
            {
                "command": "sensecode.login",
                "category": "SenseCode",
                "title": "%sensecode.command.login%",
                "icon": "$(account)"
            },
            {
                "command": "sensecode.logout",
                "category": "SenseCode",
                "title": "%sensecode.command.logout%"
            },
            {
                "command": "sensecode.settings",
                "category": "SenseCode",
                "title": "%sensecode.command.settings%",
                "icon": "$(gear)"
            },
            {
                "command": "sensecode.inlineSuggest.trigger",
                "category": "SenseCode",
                "title": "%sensecode.command.completionTrigger%"
            },
            {
                "command": "sensecode.onSuggestionAccepted",
                "category": "SenseCode",
                "title": "%sensecode.command.onSuggestionAccepted%"
            }
        ],
        "keybindings": [
            {
                "command": "sensecode.inlineSuggest.trigger",
                "key": "Alt+/"
            }
        ],
        "icons": {
            "sensecore-dark": {
                "description": "SenseCore icon",
                "default": {
                    "fontPath": "media/penrose.woff",
                    "fontCharacter": "\\E001"
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "sensecode-view-container",
                    "title": "SenseCode",
                    "icon": "media/logo1.svg"
                }
            ]
        },
        "views": {
            "sensecode-view-container": [
                {
                    "type": "webview",
                    "id": "sensecode.view",
                    "name": "%sensecode.view.title%"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint -c .eslintrc.js --ext .ts ./src --fix",
        "vscode:prepublish": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/markdown-it": "^12.2.3",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "@vscode/vsce": "^2.18.0",
        "@vscode/webview-ui-toolkit": "^1.2.0",
        "axios": "^0.24.0",
        "crypto": "^1.0.1",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "ts-loader": "^9.4.1",
        "markdown-it": "^13.0.1",
        "ts-webpack": "^1.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "zlib": "^1.0.5"
    }
}