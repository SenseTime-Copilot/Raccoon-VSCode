{
    "publisher": "SenseTime",
    "name": "sensecode",
    "displayName": "Sense Code",
    "description": "Sense Code is an AI-based coding assistant.",
    "homepage": "https://sensecore.cn",
    "repository": {
        "type": "git",
        "url": "https://gitlab.bj.sensetime.com/elementary/studio/sensecode-vscode-extension.git"
    },
    "icon": "media/sensecode-logo.png",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Machine Learning",
        "Other"
    ],
    "keywords": [
        "python",
        "go",
        "java",
        "javascript",
        "c++",
        "c#",
        "php",
        "typescript",
        "ai",
        "autocomplete",
        "inline completion"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": [
            {
                "properties": {
                    "SenseCode.Engines": {
                        "type": "array",
                        "description": "Engine List",
                        "items": {
                            "type": "object",
                            "description": "Engine Config",
                            "properties": {
                                "label": {
                                    "type": "string"
                                },
                                "url": {
                                    "type": "string"
                                },
                                "key": {
                                    "type": "string"
                                },
                                "config": {
                                    "type": "object"
                                }
                            }
                        },
                        "default": [
                            {
                                "label": "SenseCode",
                                "url": "http://10.8.33.51:15000/v1/completions/",
                                "config": {
                                    "n": 3,
                                    "max_tokens": 128,
                                    "stop": [
                                        "\n\n"
                                    ],
                                    "temperature": 0.8,
                                    "top_p": 0.95
                                }
                            },
                            {
                                "label": "OpenAI",
                                "url": "https://api.openai.com/v1/completions",
                                "key": "<YOUR OPENAI API KEY>",
                                "config": {
                                    "model": "text-davinci-003",
                                    "n": 3,
                                    "max_tokens": 128,
                                    "stop": [
                                        "\n\n"
                                    ],
                                    "temperature": 0.8,
                                    "top_p": 0.95
                                }
                            },
                            {
                                "label": "TianQi",
                                "url": "https://tianqi.aminer.cn/api/v2/multilingual_code_generate_adapt",
                                "config": {
                                    "apikey": "<YOUR TIANQI API KEY>",
                                    "apisecret": "<YOUR TIANQI API SECRET>",
                                    "n": 3,
                                    "max_tokens": 128,
                                    "stop": [
                                        "\n\n"
                                    ],
                                    "temperature": 0.8,
                                    "top_p": 0.95,
                                    "top_k": 0
                                }
                            }
                        ]
                    },
                    "SenseCode.CompletionAutomatically": {
                        "type": "boolean",
                        "default": true,
                        "description": "Automatic get completion suggestions once stop typing."
                    },
                    "SenseCode.CompletionDelay": {
                        "type": "number",
                        "minimum": 0.5,
                        "default": 0.5,
                        "description": "The delay in seconds to start getting completion suggestions. You can change this value to get better experience in coding with our extension and to avoid some unuseful completion suggestions."
                    },
                    "SenseCode.Privacy": {
                        "type": "boolean",
                        "description": "Accept sharing the generated code only for research purposes to make SenseCode better. Otherwise, the code won't be stored and is only used to assist your programming.",
                        "default": null
                    }
                }
            }
        ],
        "commands": [
            {
                "command": "sensecode.config.selectEngine",
                "category": "SenseCode",
                "title": "Select Code Completion Engine"
            },
            {
                "command": "sensecode.inlineSuggest.toggle",
                "category": "SenseCode",
                "title": "Toggle Code Inline Completion"
            },
            {
                "command": "sensecode.inlineSuggest.trigger",
                "category": "SenseCode",
                "title": "Code Inline Completion Trigger"
            }
        ],
        "keybindings": [
            {
                "command": "sensecode.inlineSuggest.trigger",
                "key": "Alt+/"
            }
        ],
        "icons": {
            "sensecore-dark": {
                "description": "SenseCore icon",
                "default": {
                    "fontPath": "media/sensecore.woff",
                    "fontCharacter": "\\E001"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@vscode/webview-ui-toolkit": "^1.2.0",
        "axios": "^0.24.0",
        "vsce": "^2.15.0"
    }
}